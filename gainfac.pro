PRO GAINFAC, LAMBDA=LAMBDA, FWHM=FWHM, S2T=S2T, T2S=T2S 
;
;; LAMBDA IS IN CM
;; FWHM IS IN ARCSEC
;
;; S2T = CONVERSION FACTOR 
;; T2S = INVERSE CONVERSION FACTOR
;
;; S = S2T * T
;; T = T2S * S


NUMER = 1.1331 * FWHM^2
DENUM = 1.5400 * LAMBDA^2
;
S2T = (NUMER/DENUM) * 1.E-03
;
T2S = 1./S2T



END
