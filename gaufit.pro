PRO GAUFIT, X,SBIN,YFIT, MOM=MOM



X1  = MIN(X)
X2  = MAX(X)


NBIN= INT((X2-X1)/SBIN)
XFIT= X1 + (X2-X1) * FINDGEN(NBIN+1)/NBIN



X0  = MEAN(X)
SIG = STDEV(X)

X    = X[SORT(X)]

NUM  = (X-X0)^2 
DNM  = 2.*SIG^2
YFIT = (1./SQRT(2.*SIG^2)) * EXP(-NUM/DNM)


;; FIND THE MOMENTS OF THE DISTRIBUTION
MOM  = MOMENT(YFIT)



END
